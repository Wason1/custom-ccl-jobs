SELECT
	P.USERNAME
	, C_CREDENTIAL_TYPE_DISP = UAR_GET_CODE_DISPLAY(C.CREDENTIAL_TYPE_CD)
	, C.END_EFFECTIVE_DT_TM

FROM
	CREDENTIAL   C
	, (LEFT JOIN PRSNL P ON (C.PRSNL_ID = P.PERSON_ID))

WHERE
	C.CREDENTIAL_TYPE_CD IN(78912298,78912302,78912347,78912075,78912208,84466793,87785665)
        /*
		78912298	PSYCHOLOGIST		Psychologist
		78912302	PHYSIO		Physiotherapist
		78912347	WO		Welfare Officer
		78912075	DIET		Dietician
		78912208	HMO		Medical Officer
		84466793	THERAPIST		Therapist
		87785665	HEALTH PSYCHOLOGIST		Health Psychologist
        */
	AND
	C.END_EFFECTIVE_DT_TM > CURDATE
	AND
	P.ACTIVE_ID = 1

WITH MAXREC = 50000, NOCOUNTER, SEPARATOR=" ", FORMAT, TIME = 30